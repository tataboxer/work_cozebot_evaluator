# 苏州科技馆 AI 助手评估平台 v3.0 环境配置

# Coze API配置
COZE_API_TOKEN=your_coze_api_token_here
COZE_BOT_ID=your_coze_bot_id_here

# LLM评估配置 (推荐火山引擎豆包)
llm_url=https://ark.cn-beijing.volces.com/api/v3/
llm_api_key=your_volcano_api_key_here
llm_model_name=doubao-1.5-pro-32k-250115

# 并发配置
DATA_PROCESSOR_THREADS=10
ASSESS_THREADS=3

# 业务系统配置 (可选)
LOGIN_HOST=your_business_system_host
LOGIN_USERNAME=encrypted_username
LOGIN_PASSWORD=encrypted_password

# 服务器配置
PORT=3000
NODE_ENV=production